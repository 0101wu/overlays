/dts-v1/;
/plugin/;

#include "rock-3-ov5647.dtsi"

/ {
	metadata {
		title ="Enable OKdo 5MP Camera";
		compatible = "radxa,rock-3c";
		category = "camera";
		description = "Enable OKdo 5MP Camera.";
	};

	/delete-node/ fragment@1;

	fragment@1 {
		target = <&i2c2>;

		__overlay__ {
			status = "okay";
		};
	};
};
