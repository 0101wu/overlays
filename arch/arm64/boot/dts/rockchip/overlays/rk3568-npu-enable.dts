/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable NPU";
		compatible = "rockchip,rk3566", "rockchip,rk3568";
		category = "misc";
		exclusive = "rknpu";
		description = "Enable NPU.";
	};

	fragment@0 {
		target = <&bus_npu>;

		__overlay__ {
			bus-supply = <&vdd_logic>;
			pvtm-supply = <&vdd_cpu>;
			status = "okay";
		};
	};

	fragment@1 {
		target = <&rknpu>;

		__overlay__ {
			memory-region = <&rknpu_reserved>;
			rknpu-supply = <&vdd_npu>;
			status = "okay";
		};
	};

	fragment@2 {
		target = <&rknpu_mmu>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@3 {
		target = <&rknpu_reserved>;

		__overlay__ {
			status = "okay";
		};
	};
};