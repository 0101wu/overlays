/dts-v1/;
/plugin/;

#include <dt-bindings/pinctrl/rockchip.h>

/ {
	metadata {
		title = "Disable FAN0";
		compatible = "radxa,cm3-io";
		category = "misc";
		exclusive = "GPIO0_C7";
		description = "Disable FAN0.";
	};
	fragment@0 {
		target = <&fan0>;

		__overlay__ {
			status = "disabled";
		};
	};

	fragment@1 {
		target = <&pwm0>;

		__overlay__ {
			status = "disabled";
		};
	};
};
