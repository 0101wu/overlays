/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM9-M0";
		compatible = "rockchip,rk3566", "rockchip,rk3568";
		category = "misc";
		description = "Enable PWM9-M0.";
	};

	fragment@0 {
		target = <&pwm9>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "active";
			pinctrl-0 = <&pwm9m0_pins>;
		};
	};
};
