/dts-v1/;
/plugin/;

#include <dt-bindings/pinctrl/rockchip.h>

/ {
	metadata {
		title = "Enable PWM3";
		compatible = "radxa,rockpis";
		category = "misc";
		exclusive = "GPIO0_C0";
		description = "Enable PWM3.\nOn Radxa ROCK PI S this is pin 13.";
	};

	fragment@0 {
		target = <&pwm3>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&pwm3_pin>;
		};
	};

	fragment@1 {
		target = <&pwm3_pin>;

		__overlay__ {
			rockchip,pins =
				<0 RK_PC0 1 &pcfg_pull_up_12ma>;
		};
	};
};
