/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable FIQ Debugger on UART9-M1";
		compatible = "rockchip,rk3566", "rockchip,rk3568";
		category = "misc";
		description = "Enable FIQ Debugger on UART9-M1.";
	};

	fragment@0 {
		target = <&fiq_debugger>;

		__overlay__ {
			status = "okay";
			rockchip,serial-id = <9>;
			pinctrl-names = "default";
			pinctrl-0 = <&uart9m1_xfer>;
		};
	};

	fragment@1 {
		target = <&uart9>;

		__overlay__ {
			status = "disabled";
		};
	};
};
